#include<stdio.h>
#include<stdlib.h>
#define N 54
void get_order(int a[N],int count);
char get_color(int a);
void print_data(int a[N]);
int main()
{
	int a[N],count;
	int i;
	scanf("%d\n",&count);
	for(i=0;i<N;i++)
		scanf("%d",&a[i]);
	get_order(a,count);
	print_data(a);	
}
void get_order(int a[N],int count)
{
	int b[N],c[N]={0},i,j;
	for(i=0;i<N;i++)
	{
		b[i]=i+1;
	}
	for(i=0;i<count;i++)
	{
		for(j=0;j<N;j++)
			c[a[j]-1]=b[j];
		for(j=0;j<N;j++)
		{
			b[j]=c[j];
		}
	}
	for(i=0;i<N;i++)
		a[i]=c[i];
}
char get_color(int a)
{
	if(a/13==0)
		return 'S';
	if(a/13==1)
		return 'H';
	if(a/13==2)
		return 'C';
	if(a/13==3)
		return 'D';
	else
		return 'J';	
}
void print_data(int a[N])
{
	int i,num;
	for(i=0;i<N;i++)
	{
		if(i!=N-1)
		{
			num=a[i]%13;
			if(!num)
				num=13;
			printf("%c%d ",get_color(a[i]),num);
		}
		else
		{
			num=a[i]%13;
			if(!num)
				num=13;
			printf("%c%d",get_color(a[i]),num);
		}
	}
}
